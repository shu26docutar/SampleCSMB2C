        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

	var g_group = new GlideRecord('sys_user_group');
	g_group.addQuery('name', '市民グループ');
	g_group.query();

	if ( g_group.next() ) {
		var gr_member = new GlideRecord('sys_user_grmember');
		gr_member.initialize();

		gr_member.group = g_group.sys_id.toString();
		gr_member.user  = current.getUniqueValue().toString();
		gr_member.insert();
	}
	

})(current, previous);]]></script>
